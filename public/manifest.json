{
	"manifest_version": 3,
	"name": "NFTea",
	"version": "1.0.0",
	"description": "A Chrome extension for staying safe while browsing NFTs",
	"action": {
		"default_popup": "index.html"
	},
	"permissions": ["activeTab"],
	"host_permissions": ["https://*.opensea.io/*"],
	"icons": {
		"48": "favicon48.png",
		"96": "favicon96.png",
		"128": "favicon128.png",
		"192": "favicon192.png",
		"512": "favicon512.png"
	}
}
